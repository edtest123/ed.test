/// <reference types="cypress" />

describe("Test springer Search funtionality", () => {
    it("Should be able to submit a successfully search and return values on the page", () => {
        //cypress code for this test goes here...
        cy.visit("https://link.springer.com/"),
        cy.get('#query').type("Biomedicine").click()
        cy.get('input[id="search"]').click()
    });
})
